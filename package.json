  "main": "config.js",
  "scripts": {
    "deploy-application": "sh ./deploy.sh",
    "build-backend": "webpack --config ./api/webpack.config.js",
    "watch-build-backend": "webpack --watch=true --config ./api/webpack.config.js",
    "start-backend": "nodemon ./api/dist/main.js localhost 8080",
    "build-frontend": "webpack --config ./frontend/webpack.config.js",
    "start-frontend": "webpack-dev-server --config ./frontend/webpack.config.js",
    "build": "concurrently --kill-others \"npm run build-backend\" \"npm run build-frontend\"",
    "dev": "concurrently --kill-others \"npm run watch-build-backend\" \"npm run start-backend\" \"npm run start-frontend\""
  },
